{
    "name": "nxt-react-app",
    "version": "4.3.0",
    "description": "Scaffolds a React + Redux (easy-peasy) App with sane webpack, babel, material-ui, styled-components and eslint defaults",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/nxtcoder17/nxt-react-app.git"
    },
    "scripts": {
        "dev": "export $(grep -E '^[^#]' env/development.env | xargs) && webpack-dev-server --progress",
        "build": "export $(grep -E '^[^#]' env/production.env | xargs) && webpack --progress"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "fs-extra": "^9.0.1",
        "ora": "^4.0.4"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@babel/runtime": "^7.11.2",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@types/react-router-dom": "^5.1.5",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.11.1",
        "babel-polyfill": "^6.26.0",
        "easy-peasy": "^3.3.1",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-madhouselabs": "1.1.2",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-react": "^1.1.7",
        "eslint-import-resolver-webpack": "^0.12.2",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-react-hooks": "^4.0.0",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-is": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "styled-components": "^5.2.0",
        "typescript": "^3.9.6",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "bin": {
        "init-react-app": "./bin/init-react-app.js"
    },
    "preferGlobal": true,
    "keywords": [
        "react",
        "init-react",
        "create-react-app",
        "nxt-react-app"
    ],
    "author": "nxtcoder17",
    "license": "ISC"
}
